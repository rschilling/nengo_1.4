<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://www.netbeans.org/ns/project/1">
    <type>org.netbeans.modules.ant.freeform</type>
    <configuration>
        <general-data xmlns="http://www.netbeans.org/ns/freeform-project/1">
            <name>nengo-simulator-ui</name>
        </general-data>
        <general-data xmlns="http://www.netbeans.org/ns/freeform-project/2">
            <!-- Do not use Project Properties customizer when editing this file manually. 
 To prevent the customizer from showing, create nbproject/project.properties file and enter 
auxiliary.show.customizer=false 
property there. Adding 
auxiliary.show.customizer.message=<message>
 will show your customized message when someone attempts to open the customizer.  -->
            <name>nengo-simulator-ui</name>
            <properties/>
            <folders>
                <source-folder>
                    <label>nengo-simulator-ui</label>
                    <location>.</location>
                    <encoding>UTF-8</encoding>
                </source-folder>
                <source-folder>
                    <label>src/java/main</label>
                    <type>java</type>
                    <location>src/java/main</location>
                    <encoding>UTF-8</encoding>
                </source-folder>
                <source-folder>
                    <label>src/python</label>
                    <type>java</type>
                    <location>src/python</location>
                    <encoding>UTF-8</encoding>
                </source-folder>
                <source-folder>
                    <label>python</label>
                    <type>java</type>
                    <location>python</location>
                    <encoding>UTF-8</encoding>
                </source-folder>
            </folders>
            <ide-actions>
                <action name="build">
                    <target>build</target>
                </action>
                <action name="clean">
                    <target>clean</target>
                </action>
                <action name="javadoc">
                    <target>javadoc</target>
                </action>
                <action name="test">
                    <target>test-simulator</target>
                </action>
                <action name="rebuild">
                    <target>clean</target>
                    <target>build</target>
                </action>
                <action name="debug">
                    <script>nbproject/ide-file-targets.xml</script>
                    <target>debug-nb</target>
                </action>
                <action name="run">
                    <target>run</target>
                </action>
            </ide-actions>
            <export>
                <type>folder</type>
                <location>bin</location>
                <build-target>build</build-target>
            </export>
            <view>
                <items>
                    <source-folder style="packages">
                        <label>src/java/main</label>
                        <location>src/java/main</location>
                    </source-folder>
                    <source-folder style="packages">
                        <label>src/python</label>
                        <location>src/python</location>
                    </source-folder>
                    <source-folder style="packages">
                        <label>python</label>
                        <location>python</location>
                    </source-folder>
                    <source-file>
                        <location>build.xml</location>
                    </source-file>
                </items>
                <context-menu>
                    <ide-action name="build"/>
                    <ide-action name="rebuild"/>
                    <ide-action name="clean"/>
                    <ide-action name="javadoc"/>
                    <ide-action name="run"/>
                    <ide-action name="test"/>
                    <ide-action name="debug"/>
                </context-menu>
            </view>
            <subprojects/>
        </general-data>
        <java-data xmlns="http://www.netbeans.org/ns/freeform-project-java/2">
            <compilation-unit>
                <package-root>src/java/main</package-root>
                <classpath mode="compile">lib/colt.jar:lib/commons-collections-3.2.jar:lib/itextpdf-5.3.4.jar:lib/jayatana-1.2.4.jar:lib/jbullet.jar:lib/jnumeric-0.1.jar:lib/jpct.jar:lib/jung-1.7.6.jar:lib/jython.jar:lib/macify-1.4.jar:lib/piccolo.jar:lib/piccolox.jar:lib/vecmath.jar:../simulator/lib/log4j-1.2.16.jar:../simulator/lib/jfreechart-1.0.1.jar:../simulator/src/java/main</classpath>
                <built-to>bin</built-to>
                <javadoc-built-to>artifacts</javadoc-built-to>
                <source-level>1.5</source-level>
            </compilation-unit>
            <compilation-unit>
                <package-root>src/python</package-root>
                <source-level>1.5</source-level>
            </compilation-unit>
            <compilation-unit>
                <package-root>python</package-root>
                <source-level>1.5</source-level>
            </compilation-unit>
        </java-data>
    </configuration>
</project>
